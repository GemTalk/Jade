!JadeiteLoginShell methodsFor!

basicCaption
	^'Jadeite Login Version: ' , self class jadeiteVersionString!

fillVersionList
	| list |
	list := self rowanReadyLibraries.
	list := list asSortedCollection: [:a :b | self isVersion: a lessThanOrEqualTo: b].
	versionListPresenter list: list!

loginFailureCaption
	^'Jadeite Login Error'!

postLogin: aGciSession
	| result |
	aGciSession verifyRowanLoaded ifFalse: [^aGciSession logout].	"Already notified the user"
	result := super postLogin: aGciSession.
	JadeiteTranscript showOnSession: aGciSession.
	^result!

updateCaption
	self caption: 'Jadeite Login Version: ', self class jadeiteVersionString! !
